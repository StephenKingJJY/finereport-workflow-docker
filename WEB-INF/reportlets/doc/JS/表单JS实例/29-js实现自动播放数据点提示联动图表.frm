<Listener event="afterinit">
<JavaScript class="com.fr.js.JavaScriptImpl">
<Parameters>
<Parameter>
<Attributes name="area"/>
<O t="XMLable" class="com.fr.base.Formula">
<Attributes>
<![CDATA[=sql("FRDemo","SELECT distinct pid FROM 地图 where pid!=''",1)]]></Attributes>
</O>
</Parameter>
</Parameters>
<Content>
<![CDATA[window.area = area;
window.temp = 0;
var len = window.area.length;
var temp = 0;
setTimeout(function() {
	var vanchart = FR.Chart.WebUtils.getChart('地图').vanCharts.charts[0]A;
	var series = vanchart.series,
		allPoints = []A;
	/* 获取chart下的所有系列和所有数据点 */
	for (var i = -1; ++i < series.length;) {
		allPoints = allPoints.concat(series[i]A.points);
	}

	setInterval(function() {
		/*刷新饼图*/
		FR.Chart.WebUtils.changeParameter(
			'饼图', {
				"area": window.area[window.temp]A
			}
		);
		/*刷新柱形图*/
		FR.Chart.WebUtils.changeParameter(
			'柱形图', {
				"area": window.area[window.temp]A
			}
		);

		for (var j = 0; j < allPoints.length; j++) {
			if (allPoints[j]A.name == window.area[window.temp]A) {
				/* 使用vanchart.showTooltip展示数据点 */
				vanchart.showTooltip(allPoints[j]A);
			}
		}

		if (window.temp == (window.area.length - 1)) {
			window.temp = 0;
		} else {
			window.temp = window.temp + 1;
		}
	}, 2000);
}, 2000);]]></Content>
</JavaScript>
</Listener>
